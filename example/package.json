{
  "name": "expo-spotify-example",
  "version": "1.0.0",
  "scripts": {
    "start": "npx expo start --dev-client",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "build": "npx expo prebuild --clean && bash prebuildscript.sh",
    "format": "prettier --write \"**/*.{json,js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "expo": "~48.0.18",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "react": "18.2.0",
    "react-native": "0.71.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/react": "~18.0.14",
    "prettier": "^3.0.1",
    "typescript": "^4.9.4"
  },
  "private": true,
  "expo": {
    "autolinking": {
      "nativeModulesDir": ".."
    }
  }
}
